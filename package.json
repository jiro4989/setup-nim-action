{
  "name": "setup-nim",
  "version": "1.0.0",
  "private": true,
  "description": "setup nim action",
  "main": "lib/setup-nim.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest",
    "update-package": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiro4989/setup-nim-actions.git"
  },
  "keywords": [
    "actions",
    "nim",
    "setup"
  ],
  "author": "GitHub",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@babel/parser": "^7.12.11",
    "child_process": "^1.0.2",
    "compare-versions": "^3.6.0",
    "fs": "0.0.1-security",
    "lodash": ">=4.17.21",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "requests": "^0.3.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.7.1",
    "@types/jest": "^26.0.24",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.2",
    "@types/semver": "^7.3.9",
    "ansi-regex": "^6.0.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.0",
    "nock": "^13.2.1",
    "npm-check-updates": "^9.1.2",
    "prettier": "^2.1.2",
    "semver-regex": "^3.1.3",
    "set-value": "^4.1.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build && npm run format",
      "post-commit": "git add node_modules/* && git commit -m \"Husky commit correct node modules\""
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  }
}
